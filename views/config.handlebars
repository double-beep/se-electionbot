<h1>{{{heading}}}</h1>

<form method="post">

    {{{data/statusText}}}

    {{#each data/configObject}}
    <div><label>{{@key}} <input type="text" name="{{@key}}" value="{{this}}" /></label></div>
    {{/each}}

    <input type="hidden" name="password" value="{{data/password}}" />
    <button>Save</button>
</form>